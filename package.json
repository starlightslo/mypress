{
  "name": "mypress",
  "version": "1.0.0",
  "description": "MyPress is simply to create a website to introduce yourself.",
  "main": "index.js",
  "scripts": {
    "test": "npm run migration-test && NODE_ENV=test node_modules/.bin/lab test/ -L -I '__core-js_shared__'",
    "test-cov-html": "npm run migration-test && NODE_ENV=test node_modules/.bin/lab test/ -r html -o coverage.html",
    "start": "TZ=UTC npm run dev",
    "dev": "npm run migration-dev && TZ=UTC NODE_ENV=dev node_modules/.bin/gulp & npm run watch:bundle",
    "production": "npm run migration-production && TZ=UTC NODE_ENV=production node_modules/.bin/gulp",
    "build": "TZ=UTC node_modules/.bin/gulp build",
    "migration-dev": "./node_modules/.bin/knex migrate:latest --env dev",
    "migration-test": "./node_modules/.bin/knex migrate:latest --env test",
    "migration-production": "./node_modules/.bin/knex migrate:latest --env production",
    "watch:bundle": "node_modules/.bin/watchify src/client/jsx/index.js -t babelify -p livereactload -o out/client/public/js/bundle.js -v"
  },
  "author": "Tony Huang <tony@huang.ninja> (http://huang.ninja)",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.8",
    "code": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^3.0.4",
    "gulp-htmlmin": "^3.0.0",
    "gulp-imagemin": "^3.2.0",
    "gulp-jshint": "^2.0.4",
    "gulp-minify-css": "^1.2.4",
    "gulp-nodemon": "^2.2.1",
    "gulp-rev-append": "^0.1.8",
    "gulp-uglify": "^2.1.2",
    "hapi-db-manager": "^1.0.3",
    "inert": "^4.2.0",
    "jshint": "^2.9.4",
    "lab": "^13.0.2",
    "livereactload": "^3.3.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-proxy": "^1.1.8",
    "react-transform": "^0.0.3",
    "sqlite3": "^3.1.8",
    "watchify": "^3.9.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.10",
    "connect-redis": "^3.2.0",
    "hapi": "^16.1.1",
    "hapi-bunyan": "^0.7.0",
    "jsonwebtoken": "^7.4.0",
    "knex": "^0.12.9",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "pg": "^6.1.5",
    "require-dir": "^0.3.1",
    "vision": "^4.1.1"
  }
}
